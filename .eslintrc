{
  "globals": {
    "document": true,
    "PropTypes": true,
    "mapboxgl": true
  },
 "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "mocha": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": [0, { "ignorePureComponents": <ignorePureComponents> }],
    "max-len":"off",
    "no-param-reassign":"off",
    "sort-keys": [
      "warn",
      "asc",
      {
        "caseSensitive": true,
        "natural": false
      }
    ],
    "no-console": [0]
  },
  "extends": "airbnb",
  "plugins": [
      "react",
      "react-native",
      "jsx-a11y",
      "import"
  ]
}
